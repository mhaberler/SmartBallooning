{
  "version": "1.0.6",
  "name": "SmartBallooning",
  "kmain": "index.js",
  "mainx": "expo-router/entry",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "patch-package",
    "start": "npx expo start --dev-client",
    "android-debug": "npx expo run:android",
    "android-release": "npx expo run:android --variant release",
    "android-release-device": "npx expo run:android --variant release --device",
    "ios-debug": "npx expo run:ios",
    "ios-release": "npx expo run:ios --configuration Release",
    "ios-device": "npx expo run:ios --device",
    "ios-release-device": "npx expo run:ios --configuration Release --device",
    "pod instal": "npx pod-install",
    "web": "npx expo start --web",
    "test": "jest --watchAll",
    "lint": "npx expo lint",
    "prebuild-ios": "npx expo prebuild --clean --platform ios",
    "prebuild-android": "npx expo prebuild --clean --platform android",
    "increment-android-version": "node incrementVersion.js",
    "push-android-testing": "cd android && fastlane beta && cd ..",
    "doctor": "npx expo-doctor",
    "build-ios-debug": "react-native build-ios --mode=Debug",
    "build-ios-release": "react-native build-ios --mode=Release",
    "deploy-android-mah": "rsync -avz android/app/build/outputs/apk  mah.priv.at:/var/www/static.mah.priv.at/apps/mobile/android/$npm_package_name",
    "npm_package_name": "echo $npm_package_name",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "ios-beta": "fastlane ios beta",
    "android-beta": "fastlane android beta",
    "build-android-release-aab": "fastlane android release_aab",
    "bump-patch": "npm version patch --no-git-tag-version && fastlane bump",
    "bump-minor": "npm version minor --no-git-tag-version && fastlane bump",
    "bump-major": "npm version major --no-git-tag-version && fastlane bump"
  },
  "react-native": {
    "net": "react-native-tcp-socket",
    "tls": "react-native-tcp-socket"
  },
  "dependencies": {
    "@generac/react-native-local-network-permission": "^1.2.0",
    "@inthepocket/react-native-service-discovery": "^0.2.0",
    "@babel/preset-env": "^7.26.0",
    "@expo-google-fonts/ubuntu": "^0.2.3",
    "@expo/config-plugins": "~10.0.0",
    "@expo/vector-icons": "^14.0.4",
    "@matthewwarnes/react-native-ble-manager-plugin": "^1.1.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "7.1.1",
    "@types/react-native-zeroconf": "^0.13.1",
    "buffer": "^6.0.3",
    "eslint": "^9.17.0",
    "expo": "^53.0.9",
    "expo-application": "~6.1.4",
    "expo-brightness": "~13.1.4",
    "expo-build-properties": "~0.14.6",
    "expo-camera": "~16.1.6",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-linking": "~7.1.5",
    "expo-location": "~18.1.5",
    "expo-router": "~5.0.7",
    "expo-sensors": "~14.1.4",
    "expo-system-ui": "~5.0.7",
    "meteojs": "^1.17.3",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-ble-manager": "^12.1.4",
    "react-native-blob-util": "^0.21.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-ota-hot-update": "^2.2.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.2.0",
    "replace-in-file": "^8.3.0",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "yargs": "^17.7.2",
    "react-native-tcp-socket": "6.2.0",
    "mqtt": "5.10.1",
    "react-native-fs": "2.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@react-native-community/cli": "latest",
    "@types/react": "~19.0.10",
    "patch-package": "^8.0.0"
  },
  "private": true
}
