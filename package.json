{
  "kmain": "index.js",
  "mainx": "expo-router/entry",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "patch-package",
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android-debug": "expo run:android --variant debug --app-id com.haberler.SBdebug",
    "android-release": "expo run:android --variant release --app-id com.haberler.SBrelease",
    "ios-debug": "expo run:ios",
    "ios-release": "expo run:ios --configuration Release",
    "ios-device": "expo run:ios --device",
    "ios-release-device": "expo run:ios  --configuration Release --device",
    "pod instal": "npx pod-install",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild-ios": "expo prebuild --clean --platform ios",
    "prebuild-ios-production": "NODE_ENV=production expo prebuild --clean --platform ios",
    "prebuild-android": "expo prebuild --clean --platform android",
    "prebuild-android-production": "NODE_ENV=production expo prebuild --clean --platform android",
    "increment-android-version": "node incrementVersion.js",
    "push-android-testing": "cd android && fastlane beta && cd ..",
    "doctor": "npx expo-doctor@latest",
    "build-ios-debug": "react-native build-ios --mode=Debug",
    "build-ios-release": "react-native build-ios --mode=Release",
    "build-android-debug-aab": "react-native build-android --mode=debug",
    "build-android-release-aab": "react-native build-android --mode=release",
    "build-android-debug-apk": "cd android && ./gradlew assembleDebug && cd ..",
    "build-android-release-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "deploy-android-mah": "rsync -avz android/app/build/outputs/apk  mah.priv.at:/var/www/static.mah.priv.at/apps/mobile/android/$npm_package_name",
    "npm_package_name": "echo $npm_package_name",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
